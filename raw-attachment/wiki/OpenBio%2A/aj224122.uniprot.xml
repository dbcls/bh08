  <reference key="21">
    <citation volume="7" type="journal article" name="Trends Plant Sci." last="560" first="555" date="2002">
      <title>The Dof family of plant transcription factors.</title>
      <authorList>
        <person name="Yanagisawa S."/>
      </authorList>
      <dbReference type="DOI" key="22" id="10.1016/S1360-1385(02)02362-2"/>
